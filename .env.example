# Copy this file to .env and fill in your values.
# Do not commit .env to version control.

# Required: Gemini API key for file search
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Flask settings (defaults shown)
FLASK_SECRET_KEY=dev-secret-change-in-production
FLASK_DEBUG=true
FLASK_HOST=0.0.0.0
FLASK_PORT=5000

# Optional: Path to store config (created by vector_generation.py)
STORE_CONFIG_FILE=store_config.json
